  object MSGroup: TMSQuery
    Connection = conMS
    SQL.Strings = (
      'select t.*, '
      
        ' (select max(l.DisplayName) from dbo.GroupList l where t.GroupId' +
        '=l.GroupId) DisplayName,'
      
        ' '#39#1055#1086#1076#1075#1088#1091#1087' '#39'+CONVERT(varchar(5),(select count(*) from dbo.GroupTr' +
        'ee g where g.ParentId=t.GroupId)) CountGrp,'
      
        ' '#39#1055#1086#1083#1100#1079#1086#1074#1072#1090#1077#1083#1077#1081' '#39'+CONVERT(varchar(5),(select count(*) from dbo.U' +
        'serTree u where u.GroupId=t.GroupId)) CountUser,'
      
        '(select count(*) from dbo.GroupTree g where g.ParentId=t.GroupId' +
        ')+'
      
        '(select count(*) from dbo.UserTree u where u.GroupId=t.GroupId) ' +
        'DelEnable'
      'from dbo.GroupTree t'
      'order by ParentId')
    RefreshOptions = [roAfterInsert, roAfterUpdate]
    UpdatingTable = 'dbo.GroupTree'
    Left = 88
    Top = 136
  end
